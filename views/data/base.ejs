<%- include ../include/head %>
<%- include ../include/nav %>
<div style="padding:0 15px;">
	<div class="row">
		<div class="col-md-12">
			<h3>
				基础统计
				<small>
					<%= key %></small>
			</h3>
			<div class="panel panel-primary">
				<div class="panel-heading">
					<div class="panel-title">访问次数 <%= list.length %></div>
				</div>
				<div class="panel-body" style="padding:0;">
					<table class="table table-striped table-hover">
						<thead>
							<tr>
								<th>访客ip</th>
								<th>主机</th>
								<th>访问url</th>
								<th>屏幕分辨率</th>
								<th>浏览器</th>
								<th>访问时间</th>
							</tr>
						</thead>
						<% for(var i=0;i<list.length;i++){ %>
						<tr>
							<td>
								<%= list[i].ip %></td>
							<td>
								<%= list[i].host %></td>
							<td>
								<%= list[i].path %></td>
							<td>
								<%= list[i].screenW %> × <%= list[i].screenH %></td>
							<td>
								<%= list[i].userAgent %></td>
							<td>
								<%= moment(list[i].date.getTime()).format("YYYY-MM-DD HH:mm:ss") %></td>

						</tr>
						<% } %></table>
				</div>
			</div>
		</div>
	</div>
</div>
<%- include ../include/foot %>